{
  "expo": {
    "name": "stableDiffusionApp",
    "slug": "stableDiffusionApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/base/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/base/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffd700"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/base/adaptive-icon.png",
        "backgroundColor": "#ffd700"
      }
    },
    "web": {
      "favicon": "./assets/base/favicon.png"
    }
  }
}
